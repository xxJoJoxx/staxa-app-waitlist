{
  "compilerOptions": {
    "target": "ES2018",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "$amplify/*": ["../.amplify/generated/*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", ".amplify"]
} 
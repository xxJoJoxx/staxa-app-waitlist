{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "$amplify/*": ["../.amplify/generated/*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", ".amplify"]
}